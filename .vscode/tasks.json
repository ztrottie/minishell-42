{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-lreadline",
                "${workspaceFolder}/src/parsing/*.c",
                "${workspaceFolder}/src/prompt/*.c",
                "${workspaceFolder}/src/env/*.c",
                "${workspaceFolder}/src/commands/*.c",
                "${workspaceFolder}/src/here_doc/*.c",
                "${workspaceFolder}/libft/libft.a",
                "-o",
                "${workspaceFolder}/minishell"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}